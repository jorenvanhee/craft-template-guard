{
    "name": "jorenvanhee/craft-template-guard",
    "description": "Password protect any page or entry in Craft CMS.",
    "version": "1.2.0",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "craftcms",
        "password"
    ],
    "license": "proprietary",
    "authors": [
        {
            "name": "Joren Van Hee",
            "homepage": "https://joren.co"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
            "jorenvanhee\\templateguard\\": "src/"
        }
    },
    "support": {
        "docs": "https://github.com/jorenvanhee/craft-template-guard",
        "source": "https://github.com/jorenvanhee/craft-template-guard",
        "issues": "https://github.com/jorenvanhee/craft-template-guard/issues"
    },
    "extra": {
        "name": "Template Guard",
        "handle": "template-guard"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-asserts": "^1.0.0",
        "vlucas/phpdotenv": "^3.0",
        "codeception/module-yii2": "^1.1",
        "codeception/module-rest": "^1.0.0"
    }
}
